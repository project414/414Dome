package shizhan;

public class Maopao {

	public static void main(String[] args) {
		int[]nums={6,4,9,2,7};
		for(int i=0;i<nums.length;i++){
			for(int j=0;j<nums.length-1;j++){
				if(nums[j]>nums[j+1]){
					int temp=nums[j];
					nums[j]=nums[j+1];
					nums[j+1]=temp;
				}
			}
		}
			for(int i=0;i<nums.length;i++){
				System.out.print(nums[i]+" ");
			}	
		

	}

}
